{
  "@type": "@builder.io/sdk:Element",
  "id": "create-lease-form",
  "component": {
    "name": "Form",
    "options": {
      "action": "createLeaseForm",
      "style": {
        "padding": "20px",
        "border": "1px solid #ccc",
        "borderRadius": "8px",
        "marginTop": "20px",
        "backgroundColor": "#ffffff"
      }
    }
  },
  "children": [
    {
      "@type": "@builder.io/sdk:Element",
      "id": "lease-title",
      "component": {
        "name": "Text",
        "options": {
          "text": "Create Lease",
          "style": { "fontSize": "18px", "fontWeight": "bold" }
        }
      }
    },
    {
      "@type": "@builder.io/sdk:Element",
      "id": "input-property-id",
      "component": {
        "name": "Form:Input",
        "options": {
          "name": "propertyId",
          "placeholder": "Property ID"
        }
      }
    },
    {
      "@type": "@builder.io/sdk:Element",
      "id": "input-tenant-id",
      "component": {
        "name": "Form:Input",
        "options": {
          "name": "tenantId",
          "placeholder": "Tenant ID"
        }
      }
    },
    {
      "@type": "@builder.io/sdk:Element",
      "id": "input-start-date",
      "component": {
        "name": "Form:Input",
        "options": {
          "name": "startDate",
          "placeholder": "Start Date (YYYY-MM-DD)"
        }
      }
    },
    {
      "@type": "@builder.io/sdk:Element",
      "id": "input-end-date",
      "component": {
        "name": "Form:Input",
        "options": {
          "name": "endDate",
          "placeholder": "End Date (YYYY-MM-DD)"
        }
      }
    },
    {
      "@type": "@builder.io/sdk:Element",
      "id": "input-rent",
      "component": {
        "name": "Form:Input",
        "options": {
          "name": "rentAmount",
          "placeholder": "Rent Amount",
          "type": "number"
        }
      }
    },
    {
      "@type": "@builder.io/sdk:Element",
      "id": "submit-lease",
      "component": {
        "name": "Form:SubmitButton",
        "options": { "text": "Add Lease" }
      }
    }
  ]
}
