  "scripts": {
    "dev": "vite dev",
    "build": "prisma db push --schema prisma/schema.prisma && vite build",
    "start": "vite preview",
    "preview": "vite preview",
    "lint": "eslint .",
    "prisma:generate": "prisma generate --schema prisma/schema.prisma",
    "prisma:push": "prisma db push --schema prisma/schema.prisma",
    "prisma:seed": "tsx prisma/seed.ts",
    "prisma:seed-demo": "node prisma/seed-demo.mjs",
    "prisma:migrate": "prisma migrate dev --name sync-relations",
    "prisma:seed:dev": "tsx prisma/seed.ts",
    "prisma:seed:prod": "tsx prisma/seed.ts",
    "reset-db": "prisma migrate reset --force --skip-generate --skip-seed && prisma db push && prisma generate && npm run prisma:seed",
    "sync": "powershell ./sync-push.ps1",
    "prisma:test": "tsx prisma/test.ts"
  },
